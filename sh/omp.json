{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "palette": {
    "black": "#262B44",
    "blue": "#4B95E9",
    "green": "#59C9A5",
    "orange": "#F07623",
    "red": "#D81E5B",
    "white": "#E0DEF4",
    "yellow": "#F3AE35"
  },
  "console_title_template": "{{ .PWD }}",
  "secondary_prompt": {
    "foreground": "p:black",
    "template": "<{{ if .Root }}p:red{{ end }},>\ue285</> "
  },
  "transient_prompt": {
    "foreground": "p:orange",
    "template": "{{ .Folder }} <white,>></><{{ if .Root }}yellow{{ else }}cyan{{ end }},>></><{{ if .Code }}red{{ else if .Root }}yellow{{ else }}cyan{{ end }},><b>></b></> "
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "upgrade",
          "style": "plain",
          "foreground": "blue",
          "template": "\uf019 "
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "{{ if .SSHSession }}ÓÆ© <yellow,>{{ .HostName }}</>:{{ end }}"
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "green",
          "template": "<{{ if .Root }}red{{ end }},>{{ .UserName }}</> <white,><b>in</b></> "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "cyan",
          "properties": {
            "style": "mixed",
            "mixed_threshold": 4
          },
          "template": "{{ .Path }} "
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "242",
          "foreground_templates": [
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}red{{ end }}"
          ],
          "properties": {
            "branch_icon": "Óú•:",
            "branch_identical_icon": "",
            "fetch_status": true,
            "ignore_submodules": {
              "*": "all"
            },
            "status_formats": {
              "Unmerged": "<p:yellow,>X%d</> ",
              "Deleted": "<p:red,>-%d</> ",
              "Added": "<p:green,>+%d</> ",
              "Modified": "<p:blue,>~%d</> ",
              "Untracked": "<218,>?%d</> "
            }
          },
          "template": "<white,><b>on</b></> <242,>{{ .HEAD }}</>{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} <#CEFDA0,>ÔÅÜ</> {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} <242,>|</>{{ end }}{{ if .Working.Changed }} <#FFEB3B,>ÔÅÑ</> {{ .Working.String }}{{ end }}{{ if .StashCount }} <cyan,>ÔÉá</>{{ end }} "
        },
        {
          "type": "fossil",
          "style": "plain",
          "foreground": "242",
          "template": "<white,><b>on</b></> \ue725 {{ .Branch }} {{ .Status.String }} "
        },
        {
          "type": "mercurial",
          "style": "plain",
          "foreground": "242",
          "properties": {
            "newprop": "Óø±"
          },
          "template": "<white,><b>on</b></> {{ .Branch }} {{ if .LocalCommitNumber }}({{ .LocalCommitNumber }}:{{ .ChangeSetIDShort }}){{ end }}{{ range .Bookmarks }} \uf02e {{ . }}{{ end }}{{ range .Tags }} \uf02b {{ . }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }} "
        },
        {
          "type": "plastic",
          "style": "plain",
          "background": "242",
          "properties": {
            "fetch_status": true,
            "branch_max_length": 25,
            "truncate_symbol": "‚Ä¶"
          },
          "template": "<white,><b>on</b></> {{ .Selector }}{{ if .Status.Changed }} ÔÅÑ {{ end }}{{ .Status.String }} "
        },
        {
          "type": "sapling",
          "style": "plain",
          "foreground": "242",
          "properties": {
            "fetch_status": true
          },
          "template": "<white,><b>on</b></> {{ if .Bookmark }}\uf097 {{ .Bookmark }}*{{ else }}\ue729 {{ .ShortHash }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }} "
        },
        {
          "type": "svn",
          "style": "plain",
          "foreground": "242",
          "properties": {
            "fetch_status": true
          },
          "template": "<white,><b>on</b></> \ue0a0{{ .Branch }} r{{ .BaseRev }} {{ .Working.String }} "
        },
        {
          "type": "text",
          "style": "plain",
          "template": "><{{ if .Root }}yellow{{ else }}cyan{{ end }},>></><{{ if .Code }}red{{ else if .Root }}yellow{{ else }}cyan{{ end }},><b>></b></> "
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "aws",
          "style": "plain",
          "foreground": "#FFA400",
          "template": "Óû≠ {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} "
        },
        {
          "type": "az",
          "style": "plain",
          "foreground": "#9ec3f0",
          "template": "ÓØò {{ .EnvironmentName }} "
        },
        {
          "type": "azfunc",
          "style": "plain",
          "foreground": "#FEAC19",
          "properties": {
            "fetch_version": true,
            "display_mode": "files"
          },
          "template": "ÔÉß {{ .Full }} "
        },
        {
          "type": "gcp",
          "style": "plain",
          "foreground": "#47888d",
          "template": "Óû≤ {{ .Project }} :: {{ .Account }} "
        },
        {
          "type": "cds",
          "style": "plain",
          "foreground": "#a7cae1",
          "template": "Óåë cds {{ .Full }} "
        },
        {
          "type": "cf",
          "style": "plain",
          "foreground": "#a7cae1",
          "template": "Ôêä cf {{ .Full }} "
        },
        {
          "type": "cftarget",
          "style": "plain",
          "foreground": "#a7cae1",
          "template": "Ôêä {{ .Org }}/{{ .Space }} "
        },
        {
          "type": "buf",
          "style": "plain",
          "foreground": "#1000D6",
          "template": "üêÉ {{ .Full }} "
        },
        {
          "type": "firebase",
          "style": "plain",
          "foreground": "#FFA000",
          "template": "Û∞•ß {{ .Project }} "
        },
        {
          "type": "helm",
          "style": "plain",
          "foreground": "#a7cae1",
          "template": "Helm {{ .Version }} "
        },
        {
          "type": "terraform",
          "style": "plain",
          "foreground": "#ebcc34",
          "template": "{{ .WorkspaceName }} "
        },
        {
          "type": "talosctl",
          "style": "plain",
          "foreground": "#FF6C00",
          "template": "Û±Éæ {{ .Context }} "
        },
        {
          "type": "pulumi",
          "style": "plain",
          "foreground": "#662d91",
          "template": "{{ .Stack }}{{ if .User }}:: {{ .User }}@{{ end }}{{ if .URL }}{{ .URL }}{{ end }} "
        },
        {
          "type": "sitecore",
          "style": "plain",
          "foreground": "white",
          "template": "Env: {{ .EndpointName }}{{ if .CmHost }} CM: {{ .CmHost }}{{ end }} "
        },
        {
          "type": "docker",
          "style": "plain",
          "foreground": "#0B59E7",
          "template": "Ôåà {{ .Context }} "
        },
        {
          "type": "nbgv",
          "style": "plain",
          "foreground": "#3a579a",
          "template": "Ôáí {{ .Version }} "
        },
        {
          "type": "project",
          "style": "plain",
          "foreground": "p:red",
          "template": "{{ if and (not .Error) (.Version) }}Ôíá {{ .Version }} {{ end }}"
        },
        {
          "type": "bazel",
          "style": "plain",
          "foreground": "#43a047",
          "template": "{{ url .Icon .URL }} {{ .Full }} "
        },
        {
          "type": "cmake",
          "style": "plain",
          "foreground": "#1E9748",
          "template": "Óòû Óòù cmake {{ .Full }} "
        },
        {
          "type": "nx",
          "style": "plain",
          "foreground": "#1976d2",
          "template": "Nx {{ .Full }} "
        },
        {
          "type": "ui5tooling",
          "style": "plain",
          "foreground": "#f5a834",
          "template": "ÔÇ≠ui5 {{ .Full }} "
        },
        {
          "type": "xmake",
          "style": "plain",
          "foreground": "#22a079",
          "template": "xmake v{{ .Full }} "
        },
        {
          "type": "crystal",
          "style": "plain",
          "foreground": "#4063D8",
          "template": "Óç∞ {{ .Full }} "
        },
        {
          "type": "dart",
          "style": "plain",
          "foreground": "#06A4CE",
          "template": "Óûò {{ .Full }} "
        },
        {
          "type": "dotnet",
          "style": "plain",
          "foreground": "#00ffff",
          "template": "Óùø {{ .Full }} "
        },
        {
          "type": "umbraco",
          "style": "plain",
          "foreground": "#d886f1",
          "template": "Û∞ïâ {{ .Version }} "
        },
        {
          "type": "elixir",
          "style": "plain",
          "foreground": "#422251",
          "template": "Óò≠ {{ .Full }} "
        },
        {
          "type": "flutter",
          "style": "plain",
          "foreground": "#06A4CE",
          "template": "Óäé {{ .Full }} "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "#7FD5EA",
          "template": "‚Ä≠Ô≥ë {{ .Full }} "
        },
        {
          "type": "haskell",
          "style": "plain",
          "foreground": "#906cff",
          "template": "Óòü {{ .Full }} "
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "#4063D8",
          "template": "Óú∏ {{ .Full }} "
        },
        {
          "type": "julia",
          "style": "plain",
          "foreground": "#4063D8",
          "template": "Óò§ {{ .Full }} "
        },
        {
          "type": "kotlin",
          "style": "plain",
          "foreground": "#906cff",
          "template": "<b>K</b> {{ .Full }} "
        },
        {
          "type": "lua",
          "style": "plain",
          "foreground": "cyan",
          "template": "Óò† {{ .Full }} "
        },
        {
          "type": "angular",
          "style": "plain",
          "foreground": "#1976d2",
          "template": "Óùì {{ .Full }} "
        },
        {
          "type": "react",
          "style": "plain",
          "foreground": "#81a1c1",
          "template": "{{ if not .Error }}Óû∫ {{ .Full }} {{ end }}"
        },
        {
          "type": "deno",
          "style": "plain",
          "foreground": "#3C82F6",
          "template": "{{ if not .Error }}Óò® {{ .Full }} {{ end }}"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "#6CA35E",
          "template": "Óúò {{ .Full }} "
        },
        {
          "type": "ocaml",
          "style": "plain",
          "foreground": "#d08770",
          "template": "Óô∫ {{ .Full }} "
        },
        {
          "type": "perl",
          "style": "plain",
          "foreground": "#4063D8",
          "template": "Óù© {{ .Full }} "
        },
        {
          "type": "php",
          "style": "plain",
          "foreground": "#4063D8",
          "template": "ÓúΩ {{ .Full }} "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:green",
          "template": "<p:yellow,>Óàµ</> {{ .Full }} "
        },
        {
          "type": "quasar",
          "style": "plain",
          "foreground": "#00B4FF",
          "template": "Ó©™ {{ .Full }}{{ if .HasVite }} Ó¨© {{ .Vite.Version }}{{ end }} "
        },
        {
          "type": "r",
          "style": "plain",
          "foreground": "blue",
          "template": "R {{ .Full }} "
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "#4063D8",
          "template": "Óûë {{ .Full }} "
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "#99908a",
          "template": "Óû® {{ .Full }} "
        },
        {
          "type": "swift",
          "style": "plain",
          "foreground": "#f6553c",
          "template": "Óùï {{ .Full }} "
        },
        {
          "type": "unity",
          "style": "plain",
          "foreground": "p:white",
          "properties": {
            "http_timeout": 2000
          },
          "template": "\ue721 {{ .UnityVersion }}{{ if .CSharpVersion }} {{ .CSharpVersion }}{{ end }} "
        },
        {
          "type": "vala",
          "style": "plain",
          "foreground": "blue",
          "template": "Óöû {{ .Full }} "
        },
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "cyan",
          "properties": {
            "threshold": 100,
            "style": "round"
          },
          "template": "<{{ if gt .Ms 86400000 }}magenta{{ else if gt .Ms 3600000 }}red{{ else if gt .Ms 60000 }}yellow{{ else if gt .Ms 1000 }}green{{ end }},>{{ .FormattedMs }} </>"
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "green",
          "foreground_templates": [
            "{{ if and (gt .Percentage 0) (lt .Percentage 30) }}red{{ end }}",
            "{{ if and (ge .Percentage 30) (lt .Percentage 50) }}yellow{{ end }}",
            "{{ if eq \"Full\" .State.String }}blue{{ end }}"
          ],
          "properties": {
            "discharging_icon": "‚á£",
            "charging_icon": "‚á°",
            "charged_icon": "‚Ä¢"
          },
          "template": "{{ if and (not .Error) (or (eq \"Full\" .State.String) (eq \"Charging\" .State.String) (and (gt .Percentage 0) (lt .Percentage 30))) }}{{ .Icon }}{{ .Percentage }}\uF295 {{ end }}"
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "darkGray",
          "properties": {
            "time_format": "15:04"
          },
          "template": "{{ .CurrentDate | date .Format }}"
        }
      ]
    }
  ]
}
